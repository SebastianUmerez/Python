def multiplos_3_o_5(n, m):
    contador = 0
    for i in range(n, m+1):
        if i % 3 == 0 or i % 5 == 0:
            contador += 1
    return contador
print(multiplos_3_o_5(1, 10))   # 5 → (3, 5, 6, 9, 10)
print(multiplos_3_o_5(5, 30))   # 10 → (5, 6, 9, 10, 12, 15, 18, 20, 21, 24, 25, 27, 30) → 13
