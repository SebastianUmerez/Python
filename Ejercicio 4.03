# Definir la función para convertir Fahrenheit a Celsius
def fahrenheit_a_celsius(F):
    C = (F - 32) * 5 / 9
    return C

# Casos de prueba
print("32°F =", fahrenheit_a_celsius(32), "°C")   # debería dar 0°C
print("212°F =", fahrenheit_a_celsius(212), "°C") # debería dar 100°C
print("68°F =", fahrenheit_a_celsius(68), "°C")   # debería dar 20°C
